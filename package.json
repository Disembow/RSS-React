{
  "name": "rss-react-task",
  "description": "RSS React Task",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "author": "Yauhen Naliotau @Disembow",
  "license": "ISC",
  "scripts": {
    "start": "ts-node server.ts",
    "cy:start-e2e": "start-server-and-test start http://localhost:666 cy:open-e2e",
    "cy:run-e2e": "cypress run --e2e cy:coverage",
    "cy:open-e2e": "cypress open --e2e --browser chrome",
    "cy:coverage": "npx nyc report --reporter=text-summary",
    "dev": "vite --port 8000 --open",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --fix . --ext .ts,.tsx",
    "unit:test": "vitest",
    "unit:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router": "^6.9.0",
    "react-router-dom": "^6.9.0",
    "sass": "^1.59.3"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/express": "^4.17.17",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-redux": "^7.1.25",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
